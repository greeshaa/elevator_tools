<% provide(:title, 'Редактирование УМ') %>
<h1>Редактирование УМ</h1>
<div class="row">
  <div class="span6 offset3">
    <%= form_for(@node) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>

      <%= f.label :name, "Название УМ:" %>
      <%= f.text_field :name %>

      <%= f.submit "Изменить", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>